apiVersion: apps/v1
kind: Deployment
metadata:
  name: pause-deployment
spec:
  replicas: 1
  selector:
    matchLabels:
      labels_foo: labels_bar
  template:
    metadata:
      labels:
        labels_foo: labels_bar
      annotations:
        annotations_foo: annotations_bar
    spec:
      containers:
      - name: pause
        image: {{.PauseImage}}
        ports:
        - containerPort: 80
